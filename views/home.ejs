    <% include partial/header %>
    <h1>Home</h1>
    <p><%= startingContent %></p>
    <% for (let post of posts) {%>
      <h1><%=post.postName%></h1>
      <p><%=post.postBody.substr(0,100)+"..."%>
      <% if(post.postBody.length>100){ %>
        <a href=<%="posts/"+post.postName%>>Read More</a>
      <%}%>
      </p>
<%}%>
    <% include partial/footer %>
